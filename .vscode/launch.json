    "version": "0.2.0",
    "configurations": [{
            "name": "osh debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/osh",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/debug",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
